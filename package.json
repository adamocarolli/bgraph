    "watch": "rollup -cw",
    "develop": "yarn build:dev --watch --config-dev-server"
  },
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@uncharted.software/grafer": "0.3.0",
    "globby": "^11.0.1",
    "glslify": "^7.1.1",
    "lit-element": "^2.4.0",
    "lit-html": "^1.3.0",
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.34.2",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.29.0",
    "run-p": "^0.0.0",
    "tweakpane": "^1.5.7",
    "typescript": "^4.1.2"

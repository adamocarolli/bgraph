{
  "name": "bgraph",
  "version": "0.1.0",
  "description": "bgraph is an in-memory browser-compatible graph database",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "build:dev": "rollup --config --config-dev",
    "clean": "run-p clean:**",
